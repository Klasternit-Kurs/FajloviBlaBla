<Window x:Class="FajloviBlaBla.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FajloviBlaBla"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
	<DockPanel>

		<UniformGrid Columns="2" DockPanel.Dock="Right">
			<Label Content="Napravljen: "/>
			<Label Name="lblNap"/>
			<Label Content="Pristup: "/>
			<Label Name="lblPri"/>
			<Label Content="Ekstenzija: "/>
			<Label Name="lblExt"/>
			
		</UniformGrid>

		<TreeView Name="tv" DataContextChanged="RadiOvo" SelectedItemChanged="PromenaSelekcije"/>
	</DockPanel>
</Window>
